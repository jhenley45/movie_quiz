
<%if @movies.present? %>
<% @movies.first[1].each do |movie| %>
<%= movie["original_title"] %><br>
<% end %>
<% end %>


<%= form_tag({controller: "casts", action: "index"}, method: "get") do | f | %>
	<%= label_tag(:movie, "Enter a movie:") %>
  <%= text_field_tag(:movie) %>
  <%= submit_tag("Go!") %>
<% end %>
