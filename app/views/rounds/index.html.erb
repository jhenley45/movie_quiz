<div class="jumbotron">
	<h1>Leaderboard</h1>
	<p><%= link_to 'New Round!', user_rounds_path(current_user), method: :post, class: "btn btn-primary btn-lg" %></p>
</div>
<table class="table">
	<th>Username</th>
	<th>Level</th>
	<th>Score</th>
	<% @rounds.each do | round | %>
		<tr>
			<td><a href="<%= user_path(User.find(round.user_id).id) %>"><%= User.find(round.user_id).username %></a></td>
			<td> <%= round.level %> </td>
			<td> <%= round.score %> </td>
		</tr>
<% end %>
</table>

</div>

<%= render "layouts/rules"%>
