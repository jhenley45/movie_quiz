<h1>Casts#index</h1>
<p>Find me in app/views/casts/index.html.erb</p>
<% @movie_names.each do |movie|%>
	<h2><%= movie.original_title %></h2>
<% end %>

<% @cast.each do |actor| %>
<%= actor["name"] %><br>
<% end %>



<%= form_tag({controller: "movies", action: "index"}, method: "get") do | f |%>
	<%= label_tag(:movie, "Enter an actor/actress:") %>
  <%= text_field_tag(:person) %>
  <%= submit_tag("Go!") %>
<% end %>

